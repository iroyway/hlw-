{
    "title": "我的互联网之路学习笔记",
    "author": "iroyway",
    "links" : {
        "sidebar" : {
            "Home" : "https://iroyway.github.io/hlw/"
        }
    },
    
    "plugins": [
        "-lunr", 
        "-search", 
        "-highlight",
        "prism",
        "code",
        "-copy-code-button",
        "search-pro",
        "splitter",
        "-chapter-fold",
        "hide-element",
        "page-toc-button",
        "back-to-top-button",
        "tbfed-pagefooter",
        "simple-page-toc"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy iroyway 2019 all right reserved.powered by Gitbook",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-solarizedlight.css"
            ]
        },
        "theme-default": {
            "showLevel": true
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}